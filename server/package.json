{
  "name": "prerender-server",
  "version": "1.0.0",
  "description": "Service for rendering webpages with headless Chrome.",
  "author": "Acorn1010",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "__comment": "tsc doesn't respect non-ts files, so we have to roll our own file copy in the build process: https://github.com/microsoft/TypeScript/issues/30835. TODO(acorn1010): try out tsc -w for start.",
    "lint": "eslint 'src/**'",
    "prebuild": "del lib/",
    "build": "tsc",
    "postbuild": "cpy '**/*' '!**/*.ts' ../lib/ --cwd=src/ --no-overwrite --parents",
    "start": "node lib/index.js"
  },
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "puppeteer": "^19.5.2",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "cpy-cli": "=3.1.1",
    "del-cli": "=3.0.1",
    "typescript": "^4.9.4"
  },
  "private": true
}
