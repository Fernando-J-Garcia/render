{
  "name": "prerender-server",
  "version": "1.0.0",
  "description": "Service for rendering webpages with headless Chrome.",
  "author": "Acorn1010",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "__comment": "tsc doesn't respect non-ts files, so we have to roll our own file copy in the build process: https://github.com/microsoft/TypeScript/issues/30835. TODO(acorn1010): try out tsc -w for start.",
    "lint": "eslint 'src/**'",
    "postinstall": "(cd ../shared && npm run build) && npm link \"render-shared-library\"",
    "prebuild": "del lib/ && (cd ../shared && npm run build) && npm link \"render-shared-library\"",
    "build": "tsc",
    "postbuild": "cpy 'src/**/*' '!**/*.ts' '!**/*.d.ts' '!**/*.d.ts.map' lib",
    "start": "node lib/index.js"
  },
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "@fastify/cors": "^8.2.1",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "ajv": "^8.12.0",
    "compression": "^1.7.4",
    "dotenv": "^16.0.3",
    "fastify": "^4.15.0",
    "firebase-admin": "^11.5.0",
    "ioredis": "^5.3.0",
    "lodash": "^4.17.21",
    "lru-cache": "^7.16.1",
    "minimatch": "^7.4.3",
    "nanoid": "^3.3.4",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.1",
    "puppeteer": "^19.6.3",
    "url": "^0.11.0",
    "zod": "^3.21.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "cpy-cli": "^4.2.0",
    "del-cli": "^5.0.0",
    "typescript": "^4.9.5"
  },
  "private": true
}
